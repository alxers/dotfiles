!URxvt*termName: rxvt-unicode-256color
URxvt*loginShell: true
URxvt.scrollBar: false
URxvt.letterSpace: -1
!URxvt.font: xft:DejaVu Sans Mono:pixelsize=14
!URxvt*font: xft:monaco:size=9
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight
URxvt*font: xft:Monospace:size=9

!Jellybeans Terminal Colours
urxvt.background: #151515
urxvt.foreground: #ffffff
! black
urxvt.color0: #888888
